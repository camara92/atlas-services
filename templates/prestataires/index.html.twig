{% extends 'base.html.twig' %}

{% block title %}Hello PrestatairesController!{% endblock %}

{% block body %}
	<div class="form_wrapper">
		<div class="form_container">
			<div class="title_container">
				<h2>
					Ajout des prestations 
				</h2>
			</div>
			<div class="row clearfix">
				<div class="">

					{{ form_start(prestation) }}
					<div class="input_field">
						{{ form_row(prestation.libelle) }}
					</div>
					<div class="input_field">  
						{{ form_row(prestation.activities) }}
					</div>
					<div class="input_field">  
						{{ form_row(prestation.adress) }}
					</div>
			
					<button type="submit" class="btn btn-success w-100"> Enregistrer </button>
                   
					{{ form_end(prestation) }}

				</div>
			</div>
		</div>
	</div>
	<p class="credit">Developed Atlas services
		<a href="http://www.designtheway.com" target="_blank">
			CAMARA Daouda
		</a>
	</p>
{% endblock %}
